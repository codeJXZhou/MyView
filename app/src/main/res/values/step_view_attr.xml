<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="VisitStepView">
        <attr name="leftMargin" format="dimension" />
        <attr name="rightMargin" format="dimension" />
        <attr name="lineWidth" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="lineColor" format="color" />
        <attr name="successColor" format="color" />
        <attr name="failColor" format="color" />
        <attr name="undoColor" format="color" />
        <attr name="dotPosition" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="successDrawable" format="reference" />
        <attr name="failDrawable" format="reference" />
        <attr name="undoDrawable" format="reference" />
    </declare-styleable>

</resources>